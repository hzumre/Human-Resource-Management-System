    <link rel="stylesheet" href="~/IK_Project.UI.styles.css" asp-append-version="true" />*@

    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>TeamThree</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- bootstrap css -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <!-- style css -->
    <link rel="stylesheet" href="~/css/style.css">
    <!-- Responsive-->
    <link rel="stylesheet" href="~/css/responsive.css">
    <!-- fevicon -->
    <link rel="icon" href="~/images/fevicon.png" type="image/gif" />
    <!-- Scrollbar Custom CSS -->
    <link rel="stylesheet" href="~/css/jquery.mCustomScrollbar.min.css">
    <!-- Tweaks for older IEs-->
    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" media="screen">
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->

</head>

<body class="main-layout">
    <!-- loader  -->
    <div class="loader_bg">
        <div class="loader"><img src="~/images/loading.gif" alt="#" /></div>
    </div>
    <!-- end loader -->
    <!-- header -->
    <header>
        <!-- header inner -->

        <nav class="navbar navbar-expand-xl navbar-toggleable-sm navbar-black bg-black border-bottom box-shadow mb-3" style="max-width:100%">


            <ul class="navbar-nav flex-grow-1">
                <li class="nav-item">
                    <img src="~/images/logoyeni.png" alt="#" width="60" height="60" style="padding-bottom:10px" />

                </li>

                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index" style="font-size:large">Home Page</a>

                </li>
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy" style="font-size:large">About</a>
                </li>
            </ul>


            <!-- Right navbar links -->
            <ul class="order-1 order-md-3 navbar-nav navbar-no-expand ml-auto">
                @if (User.Identity.IsAuthenticated)
                {
                    @if (User.IsInRole("Admin"))
                    {
                        <li class="navbar-item">
                            <a class="nav-link text-dark" asp-controller="Home" style="font-size:large" asp-action="Index" asp-area="Admin">Admin Home Page</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle nav-link text-dark" asp-controller="Home" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="font-size:large" asp-action="Index" asp-area="Admin">Admin Easy Menu</a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item" asp-action="Create" asp-area="Admin" asp-controller="Admin">Admin Transactions</a>
                                <a class="dropdown-item" asp-action="Index" asp-area="Admin" asp-controller="Company">Company Transactions</a>
                                <a class="dropdown-item" asp-action="Index" asp-area="Admin" asp-controller="CompanyManager">Company Manager Transactions</a>
                                <a class="dropdown-item" asp-action="Index" asp-area="Admin" asp-controller="Menu">Package Transactions</a>
                                <a class="dropdown-item" asp-action="Index" asp-area="Admin" asp-controller="Role">Role Transactions</a>
                                <a class="dropdown-item" asp-action="ListUsers" asp-area="Admin" asp-controller="Role">User-Role Transactions</a>

                                @*     <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="#">Something else here</a>*@
                            </div>
                        </li>

                        <li class="navbar-item">
                            <a class="nav-link text-dark" asp-area="Admin" style="font-size:large" asp-controller="Home" asp-action="ProfileDetails" asp-route-userName="@User.Identity.Name">Profile Details</a>
                        </li>

                    }

                    @if (User.IsInRole("CompanyManager"))
                    {
                        <li class="navbar-item">
                            <a class="nav-link text-dark" asp-area="CompanyManager" style="font-size:large" asp-controller="Home" asp-action="Index" asp-route-userName="@User.Identity.Name">Manager Home Page</a>


                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle nav-link text-dark" asp-controller="Home" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="font-size:large" asp-action="Index" asp-controller="Home" asp-area="CompanyManager">Manager Easy Menu</a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item" asp-action="Index" asp-area="CompanyManager" asp-controller="CompanyManager">Company Manager transactions</a>
                                <a class="dropdown-item" asp-action="Index" asp-area="CompanyManager" asp-controller="Employee">Employee transactions</a>
                                <a class="dropdown-item" asp-area="CompanyManager" asp-action="CheckAdvanceDemands" asp-controller="Advance">Check Advance Demands</a>
                                <a class="dropdown-item" asp-action="CheckExpenseDemands" asp-controller="Expense" asp-area="CompanyManager">Check Expense Demands</a>
                                <a class="dropdown-item" asp-area="CompanyManager" asp-action="CheckPermissionDemands" asp-controller="Permission">Check Permission Demands</a>


                                @*     <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="#">Something else here</a>*@
                            </div>
                        </li>

                        <li class="navbar-item">
                            <a class="nav-link text-dark" asp-area="CompanyManager" style="font-size:large" asp-controller="Home" asp-action="ProfileDetails" asp-route-userName="@User.Identity.Name">Profile Details</a>
                        </li>
                    }

                    @if (User.IsInRole("Employee"))
                    {
                        <li class="navbar-item">
                            <a class="nav-link text-dark" asp-controller="Home" style="font-size:large" asp-action="Index" asp-area="Employee">Employee Home Page</a>
                        </li>

                        <li class="nav-item dropdown">

                            <a class="nav-link dropdown-toggle nav-link text-dark" asp-controller="Home" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="font-size:large" asp-action="Index" asp-controller="Home" asp-area="Employee">Employee Easy Menu</a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">


                                <a class="dropdown-item" asp-action="GetMyAllExpenses" asp-area="Employee" asp-controller="Expense">Expense transactions</a>
                                <a class="dropdown-item" asp-action="GetMyAllAdvances" asp-area="Employee" asp-controller="Advance">Advance transactions</a>
                                <a class="dropdown-item" asp-action="GetMyAllPermission" asp-area="Employee" asp-controller="Permission">Permission transactions</a>


                                @*     <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="#">Something else here</a>*@
                            </div>
                        </li>

                        <li class="navbar-item">
                            <a class="nav-link text-dark" asp-area="Employee" style="font-size:large" asp-controller="Home" asp-action="ProfileDetails" asp-route-userName="@User.Identity.Name">Profile Details</a>
                        </li>
                    }

                    <li class="navbar-item">
                        <a class="nav-link text-danger" asp-area="Admin" style="font-size:large; color:darkblue" asp-controller="Home" asp-action="logOut">@User.Identity.Name, Log Out</a>
                    </li>
                }
                else
                {
                    <li class="navbar-item">
                        <a class="nav-link text-dark" style="font-size:medium" style="font-size:large" asp-area="Admin" asp-controller="Home" asp-action="Login" asp-route-Name="@User.Identity.Name">Log In</a>
                    </li>
                    <li class="navbar-item">
                        <a class="nav-link text-dark" style="font-size:medium" style="font-size:large" asp-area="Admin" asp-controller="Home" asp-action="Register">Register</a>
                    </li>

                }
            </ul>
        </nav>



    </header>
    <!-- end header inner -->
    <!-- end header -->
    <!-- banner -->

    <!-- end banner -->

    <!-- service -->
    <div id="services" class="service">
        <div class="container-fluid">
            <div class="row d_flex">
                <div class="container">
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>
            </div>
        </div>
    </div>
    <!-- end service -->
    <!-- solutions -->
    <div class="solutions">
        <div class="container">
            <div class="row d_flex">
                <div class="col-md-7">
                    <div class="titlepage">
                        <h2>Solutions for every <br>Specific need</h2>
                        <span>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five</span>
                        <a class="read_more" href="Javascript:void(0)">Read More</a>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="solutions_img">
                        <figure><img src="~/images/solusan.png" alt="#" /></figure>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end solutions -->
    <!-- work -->
    <div id="work" class="work">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="titlepage">
                        <h2>See More Our Work</h2>
                        <span>It is a long established fact that a reader will be distracted by the readable content of a page when looking at its layout. The point of using Lorem Ipsum is that it has a istribution of letters, content here', making it look like readable English. Many desktop publishing packages </span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="our_work">
                        <figure><img src="~/images/work1.jpg" alt="#" /></figure>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="our_work">
                        <figure><img src="~/images/work2.jpg" alt="#" /></figure>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="our_work">
                        <figure><img src="~/images/work3.jpg" alt="#" /></figure>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="our_work">

                        <figure><img src="~/images/work4.jpg" alt="#" /></figure>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end work -->

    <!--  footer -->
    <footer>
        <div class="footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-5">
                        <div class="multipurpose">
                            <h3>Let’s  <br>Talk <br> Business</h3>
                        </div>
                    </div>
                    <div class="col-md-7 sm_none">
                        <div class="contac_detel">
                            <ul>
                                <li><img src="~/images/call.png" alt="#" />+91 1234567890</li>
                                <li><a href="Javascript:void(0)"> <img src="~/images/gmail.png" alt="#" />demo@gmail.com</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="multipurpose">
                            <p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the Lorem Ipsum is simply dummy text of the printing and typesetting industry. </p>
                        </div>
                    </div>
                    <div class="col-md-7 sm_show">
                        <div class="contac_detel">
                            <ul>
                                <li><img src="images/call.png" alt="#" />+91 1234567890</li>
                                <li><a href="Javascript:void(0)"> <img src="~/images/gmail.png" alt="#" />demo@gmail.com</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <p>Copyright 2020 All Right Reserved By<a href="https://html.design/"> Free Html Templates</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    @await RenderSectionAsync("Scripts", required: false)
    <!-- end footer -->
    <!-- Javascript files-->
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/jquery-3.0.0.min.js"></script>
    <script src="~/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/js/custom.js"></script>
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/jquery-validate/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
</body>

</html>

